  "context_menu": {
    "id": "context_menu",
    "type": "context_menu",
    "items": {
      "contract_list_item": [
        {
          "id": "view_contract",
          "label": "View Contract",
          "icon": "EyeIcon",
          "action": "navigate",
          "target": "/contracts/{{id}}",
          "permissions": [
            "contract:view"
          ]
        },
        {
          "id": "edit_contract",
          "label": "Edit Contract",
          "icon": "EditIcon",
          "action": "navigate",
          "target": "/contracts/{{id}}/edit",
          "permissions": [
            "contract:edit"
          ],
          "conditions": {
            "status": [
              "DRAFT",
              "PENDING_CONFIGURATION"
            ]
