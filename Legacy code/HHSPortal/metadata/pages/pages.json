      },
      "layout_wrapper": "AppLayout",
      "layout_ref": "contract_create_layout",
      "permissions": ["contract:create"],
      "roles": ["ACCO_MANAGER", "ACCO_STAFF"],
      "global_actions": {
        "enabled": true,
        "position": "sticky_bottom",
        "actions": ["save", "cancel", "close"],
        "form_state_tracking": true,
        "unsaved_changes_warning": true,
        "auto_save": {
          "enabled": false,
          "interval": 30000
        }
