  "api_endpoints": {
    "contracts": {
      "list": {
        "endpoint": "/api/contracts",
        "method": "GET",
        "description": "Get paginated list of contracts",
        "parameters": {
          "page": {"type": "integer", "default": 1, "description": "Page number"},
          "limit": {"type": "integer", "default": 25, "description": "Items per page"},
          "status": {"type": "string", "description": "Filter by status"},
          "agency_id": {"type": "uuid", "description": "Filter by agency"},
          "provider_id": {"type": "uuid", "description": "Filter by provider"},
          "contract_type_id": {"type": "uuid", "description": "Filter by contract type"},
          "search": {"type": "string", "description": "Search query"},
          "sort": {"type": "string", "description": "Sort field and direction"},
          "start_date_from": {"type": "date", "description": "Filter by start date from"},
          "start_date_to": {"type": "date", "description": "Filter by start date to"},
          "amount_from": {"type": "decimal", "description": "Filter by amount from"},
          "amount_to": {"type": "decimal", "description": "Filter by amount to"}
        }
      },
      "detail": {
        "endpoint": "/api/contracts/{id}",
        "method": "GET",
        "description": "Get specific contract details"
      },
      "create": {
        "endpoint": "/api/contracts",
        "method": "POST",
        "description": "Create new contract"
      },
      "update": {
        "endpoint": "/api/contracts/{id}",
        "method": "PUT",
        "description": "Update existing contract"
      },
      "delete": {
        "endpoint": "/api/contracts/{id}",
        "method": "DELETE",
        "description": "Delete contract"
      },
      "status_change": {
        "endpoint": "/api/contracts/{id}/status",
        "method": "PUT",
        "description": "Change contract status"
      },
      "amendments": {
        "list": {
          "endpoint": "/api/contracts/{id}/amendments",
          "method": "GET",
          "description": "Get contract amendments"
        },
        "create": {
          "endpoint": "/api/contracts/{id}/amendments",
          "method": "POST",
          "description": "Create new amendment"
        }
      },
      "export": {
        "endpoint": "/api/contracts/export",
        "method": "GET",
        "description": "Export contracts to various formats"
      }
    }
  }
