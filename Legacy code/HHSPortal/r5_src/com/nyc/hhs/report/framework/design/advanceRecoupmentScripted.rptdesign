  <Script>function beforeDrawAxisLabel( axis, label, icsc )&#xD;
{ &#xD;
       if (!axis.isCategoryAxis()){&#xD;
          localValue = label.getCaption().getValue();&#xD;
          if(localValue >= 0)&#xD;
          	label.getCaption().setValue(&quot;$&quot;+changeFormat(localValue));&#xD;
          else&#xD;
          	label.getCaption().setValue(&quot;$(&quot;+changeFormat(localValue)+&quot;)&quot;);&#xD;
       }&#xD;
} &#xD;
&#xD;
&#xD;
function changeFormat(data){&#xD;
       localFloatValue = parseFloat(data);&#xD;
       localReturnValue = &quot;&quot;;&#xD;
       if(localFloatValue >= 1000000000){&#xD;
              localReturnValue = localFloatValue/1000000000 +&quot;B&quot;;&#xD;
       }else if(localFloatValue >= 1000000 &amp;&amp; localFloatValue &lt; 1000000000){&#xD;
              localReturnValue = localFloatValue/1000000 +&quot;M&quot;;&#xD;
       }else if(localFloatValue >= 1000 &amp;&amp; localFloatValue &lt; 1000000){&#xD;
              localReturnValue = localFloatValue/1000 +&quot;K&quot;;&#xD;
       }else{&#xD;
              localReturnValue = localFloatValue;&#xD;
       }&#xD;
       return localReturnValue;&#xD;
}&#xD;
&#xD;
/**&#xD;
 * Called before generation of chart model to GeneratedChartState.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeGeneration( chart, icsc )&#xD;
{&#xD;
	var fyYearId = icsc.getExternalContext().getScriptable().getParameterValue(&quot;fyYearId&quot;);&#xD;
	var title = chart.getTitle().getLabel().getCaption().getValue();&#xD;
	title = title+&quot; - FY &quot;+fyYearId;&#xD;
 	chart.getTitle().getLabel().getCaption().setValue(title);&#xD;
}</Script>
