          "is_final": true,
          "is_error": false,
          "state_type": "manual",
          "permissions": {
            "view": ["ACCO_MANAGER", "ACCO_STAFF", "FINANCE_MANAGER", "FINANCE_STAFF"]
          },
          "actions": [],
          "attributes": {
            "color": "#10B981",
            "icon": "check",
            "priority": "normal"
          }
        },
        {
          "id": "invoice_rejected",
          "name": "Invoice Rejected",
          "display_name": "Invoice Rejected",
          "description": "Invoice has been rejected",
          "is_initial": false,
          "is_final": true,
          "is_error": true,
          "state_type": "manual",
          "permissions": {
            "view": ["ACCO_MANAGER", "ACCO_STAFF", "FINANCE_MANAGER", "FINANCE_STAFF"]
          },
          "actions": [],
          "attributes": {
            "color": "#EF4444",
            "icon": "x",
            "priority": "high"
          }
        }
      ],
      "transitions": [
        {
          "from": "invoice_submitted",
          "to": "pending_invoice_review",
          "action": "submit_invoice_review",
          "conditions": ["invoice_valid"],
          "async": false
        },
        {
          "from": "pending_invoice_review",
          "to": "invoice_approved",
          "action": "approve_invoice",
          "conditions": ["invoice_approved_conditions"],
          "async": false
        },
        {
          "from": "pending_invoice_review",
          "to": "invoice_rejected",
          "action": "reject_invoice",
          "conditions": ["invoice_rejected_conditions"],
          "async": false
        }
      ],
      "business_rules": [
        {
          "id": "invoice_valid",
          "name": "Invoice Valid",
          "description": "Invoice must be valid and complete",
          "expression": "{{invoice.amount}} > 0 && {{invoice.invoice_number}} && {{invoice.invoice_date}}"
        },
        {
          "id": "invoice_approved_conditions",
          "name": "Invoice Approved Conditions",
          "description": "Conditions for invoice approval",
          "expression": "{{invoice.amount}} <= {{contract.budget_amount}} && {{invoice.amount}} > 0"
        },
        {
          "id": "invoice_rejected_conditions",
          "name": "Invoice Rejected Conditions",
          "description": "Conditions for invoice rejection",
          "expression": "{{invoice.amount}} > {{contract.budget_amount}} || {{invoice.amount}} <= 0"
        }
      ],
      "notifications": [
        {
          "event": "workflow_started",
          "template": "invoice_review_started",
          "recipients": ["ACCO_MANAGER", "FINANCE_MANAGER"]
        },
        {
          "event": "invoice_approved",
          "template": "invoice_approved",
          "recipients": ["ACCO_MANAGER", "ACCO_STAFF"]
        },
        {
          "event": "invoice_rejected",
          "template": "invoice_rejected",
          "recipients": ["ACCO_MANAGER", "ACCO_STAFF"]
        }
      ]
    }
  },
