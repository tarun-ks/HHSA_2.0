<?xml version="1.0" encoding="UTF-8"?>
<!--
    The "borderless" skin theme simply overrides selective skin files.  This configuration file sets up the
    search paths and overrides for such skin files.  All other configuration elements are unnecessary.
-->
<skin
    xmlns="http://www.bea.com/servers/portal/framework/laf/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bea.com/servers/portal/framework/laf/1.0.0 laf-skin-1_0_0.xsd"
>
    <images>
        <search-path>
            <path-element>../images</path-element>
        </search-path>
    </images>
    <render-dependencies>
        <html>
            <links>
                <search-path>
                    <path-element>css</path-element>
                </search-path> 
            </links>
        </html>
    </render-dependencies>
</skin>
