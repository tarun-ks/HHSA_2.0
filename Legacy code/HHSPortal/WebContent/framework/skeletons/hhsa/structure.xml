<?xml version="1.0" encoding="UTF-8"?>
<!--
    Two specializations are provided for the "bighorn" skeleton:
    
    The "msie" classification is used for ALL versions if Microsoft Internet Explorer. It provides
    browser-specific workarounds for use in CSS-based layouts.
    
    The "borderless" theme renders portlets without decoration; the titlebar (including buttons), border
    and background are all suppressed.  The skeleton theme suppresses the titlebar - see the skin theme
    for border and background suppression functionality.
-->
<structure 
    xmlns="http://www.bea.com/servers/portal/framework/laf/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bea.com/servers/portal/framework/laf/1.0.0 laf-structure-1_0_0.xsd"
>
    <specializations>
        <classifications>
            <classification name="msie" path="msie"/>
        </classifications>
    </specializations>
    <themes>
        <theme name="borderless" path="borderless"/>
    </themes>
</structure>
