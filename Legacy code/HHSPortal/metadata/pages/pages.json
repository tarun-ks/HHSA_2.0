        "updated_at": "2024-01-01T00:00:00Z"
      }
    },
    {
      "id": "contract_list",
      "name": "ContractList",
      "type": "list",
      "object": "contract",
      "route": "/contracts",
      "display": {
        "opening_mode": "page",
        "icon": "List",
        "title": "Contracts",
        "subtitle": "Manage and track all contracts",
        "breadcrumbs": [
          "Home",
          "Contract Management",
          "Contracts"
        ]
      },
      "layout_wrapper": "AppLayout",
      "layout_ref": "contract_list_layout",
      "permissions": ["contract:view"],
      "roles": ["ACCO_MANAGER", "ACCO_STAFF", "ACCO_ADMIN_STAFF", "FINANCE_MANAGER", "FINANCE_STAFF", "PROGRAM_MANAGER", "PROVIDER_MANAGER", "PROVIDER_STAFF"],
      "query_params": {
        "status": {
          "type": "string",
          "multiple": true,
          "description": "Filter by contract status"
        },
        "agency_id": {
          "type": "uuid",
          "multiple": true,
          "description": "Filter by agency"
        },
        "provider_id": {
          "type": "uuid",
          "multiple": true,
          "description": "Filter by provider"
        },
        "contract_type_id": {
          "type": "uuid",
          "multiple": true,
          "description": "Filter by contract type"
        },
        "search": {
          "type": "string",
          "description": "Search query"
        },
        "start_date_from": {
          "type": "date",
          "description": "Filter by start date from"
        },
        "start_date_to": {
          "type": "date",
          "description": "Filter by start date to"
        },
        "amount_from": {
          "type": "decimal",
          "description": "Filter by amount from"
        },
        "amount_to": {
          "type": "decimal",
          "description": "Filter by amount to"
        },
        "sort": {
          "type": "string",
          "description": "Sort field and direction",
          "default": "created_date:desc"
        },
        "page": {
          "type": "integer",
          "description": "Page number",
          "default": 1
        },
        "limit": {
          "type": "integer",
          "description": "Items per page",
          "default": 25
        }
      },
      "metadata": {
        "description": "List view for contracts with filtering and actions",
        "keywords": ["contracts", "list", "filter", "search", "management"],
        "created_at": "2024-01-01T00:00:00Z",
        "updated_at": "2024-01-01T00:00:00Z"
