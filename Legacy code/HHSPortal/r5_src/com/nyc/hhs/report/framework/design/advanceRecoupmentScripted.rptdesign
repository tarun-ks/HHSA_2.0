            <text-property name="content"><![CDATA[<div id="advrecoupReportToolTip" style="display: none;position:absolute;border: 2px solid #73AD21;padding: 10px;background-color:white;opacity:1;filter: alpha(opacity=100);">
	<div id="advrecouptooltipTitle"></div>
	<div><span id="advrecouptooltipValue"></span></div>
	<div><span id="advrecouptooltipCategory"></span></div>
</div>
<script>
function advrecouptoolTipSeries(event,percent,categoryData,valueData, valueSeriesName,x,y){
var colorMap = new Array();
colorMap["Advanced"]="rgb(80,166,218)";
colorMap["Recouped"]="rgb(232,172,57)";
	Pp = document.getElementById("advrecoupReportToolTip");
   	Pp.style.display="block";
   	Pp.style.left = getCordinates(event).x + 5 +"px";
	Pp.style.top = getCordinates(event).y + 5 +"px";
	Pp.style.borderColor = colorMap[valueSeriesName];
	if(valueSeriesName=="Recouped")
		document.getElementById("advrecouptooltipValue").innerHTML= "<b>$"+valueData.toFixed(2).replace(/./g, function(c, i, a) {
    return i &amp;&amp; c !== "." &amp;&amp; ((a.length - i) % 3 === 0) ? ',' + c : c;
})+" - "+percent.toFixed(2)+"%</b>";

else
	document.getElementById("advrecouptooltipValue").innerHTML= "<b>$"+valueData.toFixed(2).replace(/./g, function(c, i, a) {
    return i &amp;&amp; c !== "." &amp;&amp; ((a.length - i) % 3 === 0) ? ',' + c : c;
})+"</b>"
	document.getElementById("advrecouptooltipCategory").innerHTML= "<b>"+categoryData+"</b>";
	document.getElementById("advrecouptooltipTitle").innerHTML= "<b>"+valueSeriesName+"</b>";
}

function advrecoupCloseToolTip(){
	document.getElementById("advrecoupReportToolTip").style.display ="none";
	document.getElementById("advrecouptooltipValue").innerHTML= "";
	document.getElementById("advrecouptooltipCategory").innerHTML= "";
	document.getElementById("advrecouptooltipTitle").innerHTML= "";
}

function getCordinates(e) {
    if( !e) return {x:0,y:0};
    if( e.pageX || e.pageY) {
        return {x:e.pageX,y:e.pageY};
    }
    if( e.clientX || e.clientY) {
        return {
            x:e.clientX,
            y:e.clientY
        };
    }
    return {x:0,y:0};
};
</script>]]></text-property>
