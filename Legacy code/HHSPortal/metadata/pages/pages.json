        "enabled": true,
        "position": "sticky_bottom",
        "actions": ["save", "cancel", "close"],
        "form_state_tracking": true,
        "unsaved_changes_warning": true,
        "auto_save": {
          "enabled": false,
          "interval": 30000
        }
      },
      "conditions": {
        "status": ["{{references.contract_statuses.DRAFT.code}}", "{{references.contract_statuses.PENDING_CONFIGURATION.code}}"]
      },
      "params": {
        "id": {
          "type": "uuid",
          "required": true,
          "description": "Contract ID"
