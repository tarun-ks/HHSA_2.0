                           skeletonUri="/framework/skeletons/hhsa/agencyhomelayout.jsp"  htmlLayoutUri="/framework/markup/layout/twocolumnlayout.html.txt"
                            id="agencyR1Layout">
            <netuix:placeholder title="left" description="The left most placeholder in this layout."
                                usingFlow="true" width="49%"  
                                markupType="Placeholder" markupName="agencyR1Layout_first">
            </netuix:placeholder>
            <netuix:placeholder title="right" description="The right most placeholder in this layout." 
                                usingFlow="true" width="49%" 
                                markupType="Placeholder" markupName="agencyR1Layout_second">
