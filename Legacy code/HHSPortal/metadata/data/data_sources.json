      },
      "response_mapping": {
        "value_field": "id",
        "label_field": "name",
        "description_field": "legal_name",
        "additional_fields": {
          "type": "type",
          "ein_tin": "ein_tin",
          "duns_number": "duns_number",
          "phone": "phone",
          "email": "email"
        }
      },
      "caching": {
        "enabled": true,
        "ttl": 3600,
        "refresh_on": ["organization_created", "organization_updated", "organization_deleted"]
