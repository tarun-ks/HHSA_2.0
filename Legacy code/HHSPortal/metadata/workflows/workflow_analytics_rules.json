{
  "version": "2.0.0",
  "description": "Workflow Analytics Rules - WF301-WF316 from System",
  "analytics_rules": {
    "contract_lifecycle_analytics": {
      "id": "contract_lifecycle_analytics",
      "name": "Contract Lifecycle Analytics",
      "description": "Analytics rules for contract lifecycle from system",
      "version": "1.0.0",
      "is_active": true,
      "created_by": "system",
      "created_at": "2024-01-01T00:00:00Z",
      "metrics": [
        {
          "id": "contract_creation_rate",
          "name": "Contract Creation Rate",
          "description": "Number of contracts created per month",
          "metric_type": "count",
          "aggregation": "monthly",
          "dimensions": ["agency_id", "contract_type_id", "status_id"],
          "filters": [
            {
              "field": "contract.created_at",
              "operator": "greater_than",
              "value": "{{current_date}} - 30 days"
            }
          ],
          "visualization": "line_chart",
          "priority": "high"
        },
        {
          "id": "contract_completion_rate",
          "name": "Contract Completion Rate",
          "description": "Percentage of contracts completed successfully",
          "metric_type": "percentage",
          "aggregation": "monthly",
          "dimensions": ["agency_id", "contract_type_id"],
          "filters": [
            {
              "field": "contract.status_id",
              "operator": "equals",
              "value": "69"
            }
          ],
          "visualization": "donut_chart",
          "priority": "high"
        },
        {
          "id": "contract_duration_analysis",
          "name": "Contract Duration Analysis",
          "description": "Average time from contract creation to completion",
          "metric_type": "duration",
          "aggregation": "average",
          "dimensions": ["agency_id", "contract_type_id"],
          "filters": [
            {
              "field": "contract.status_id",
              "operator": "equals",
              "value": "69"
            }
          ],
          "visualization": "bar_chart",
          "priority": "medium"
        },
        {
          "id": "contract_status_distribution",
          "name": "Contract Status Distribution",
          "description": "Distribution of contracts by status",
          "metric_type": "count",
          "aggregation": "current",
          "dimensions": ["status_id"],
          "filters": [],
          "visualization": "pie_chart",
          "priority": "high"
        }
      ]
    },
    "budget_utilization_analytics": {
      "id": "budget_utilization_analytics",
      "name": "Budget Utilization Analytics",
      "description": "Analytics rules for budget utilization from system",
      "version": "1.0.0",
      "is_active": true,
      "created_by": "system",
      "created_at": "2024-01-01T00:00:00Z",
      "metrics": [
        {
          "id": "budget_utilization_rate",
          "name": "Budget Utilization Rate",
          "description": "Percentage of budget utilized per contract",
          "metric_type": "percentage",
          "aggregation": "monthly",
          "dimensions": ["agency_id", "contract_type_id"],
          "filters": [
            {
              "field": "contract.budget_amount",
              "operator": "greater_than",
              "value": "0"
            }
          ],
          "visualization": "gauge_chart",
          "priority": "high"
        },
        {
          "id": "budget_variance_analysis",
          "name": "Budget Variance Analysis",
          "description": "Variance between planned and actual budget",
          "metric_type": "variance",
          "aggregation": "monthly",
          "dimensions": ["agency_id", "contract_type_id"],
          "filters": [
            {
              "field": "contract.budget_amount",
              "operator": "greater_than",
              "value": "0"
            }
          ],
          "visualization": "bar_chart",
          "priority": "medium"
        },
        {
          "id": "budget_approval_time",
          "name": "Budget Approval Time",
          "description": "Average time for budget approval",
          "metric_type": "duration",
          "aggregation": "average",
          "dimensions": ["agency_id", "contract_type_id"],
          "filters": [
            {
              "field": "workflow.workflow_id",
              "operator": "equals",
              "value": "WF304"
            }
          ],
          "visualization": "line_chart",
          "priority": "medium"
        }
      ]
    },
