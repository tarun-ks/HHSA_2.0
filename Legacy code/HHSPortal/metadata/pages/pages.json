    {
      "id": "contract_edit",
      "name": "ContractEdit",
      "type": "form",
      "object": "contract",
      "route": "/contracts/:id/edit",
      "display": {
        "opening_mode": "page",
        "icon": "Edit",
        "title": "Edit Contract",
        "subtitle": "Edit contract details",
