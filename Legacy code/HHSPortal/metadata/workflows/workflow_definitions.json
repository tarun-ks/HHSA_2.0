        },
        {
          "from": "pending_cof",
          "to": "cof_approved",
          "action": "approve_cof",
          "conditions": ["funds_available"],
          "async": false
        },
        {
          "from": "pending_cof",
          "to": "cof_rejected",
          "action": "reject_cof",
          "conditions": ["funds_unavailable"],
          "async": false
        }
      ],
      "business_rules": [
        {
          "id": "contract_has_budget",
          "name": "Contract Has Budget",
          "description": "Contract must have budget information before CoF submission",
          "expression": "{{contract.budget_amount}} > 0"
        },
        {
          "id": "funds_available",
          "name": "Funds Available",
          "description": "Sufficient funds must be available for the contract",
          "expression": "{{contract.budget_amount}} <= {{available_funds}}"
        },
        {
          "id": "funds_unavailable",
          "name": "Funds Unavailable",
          "description": "Insufficient funds for the contract",
          "expression": "{{contract.budget_amount}} > {{available_funds}}"
        }
      ],
